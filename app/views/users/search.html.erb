<% @users.each do |user| %>
<% user.posts.each do |post| %>
  
     <div id="post_content">
     <div class="card">
    <h4 class "title" ><%= link_to post.title, post, {style: "color: black"} %></h4>
     <i class="small material-icons">loyalty</i>

    <% post.tags.any? %>
     <% post.tags.each do |tag| %>
     <div class="chip">
        <%= link_to tag.name, tag_path(tag.name)%>
     </div>  
<% end %>
    <p class="author">Submitted <%= time_ago_in_words(post.created_at) %> 
     <% if post.user.name? %>
     by <%=  post.user.name%> 
     <% else %>
     by Incognito 
     <% end %>
    </p>ï»¿

  <div class="chip"><%= image_tag(post.user.avatar.url(:small))  %></div>
   <div class="btn-group", style="padding-top: 2%;">
 
      <%= link_to like_post_path(post), method: :put, class: "btn btn-default btn-sm" do %>
        <span class="glyphicon glyphicon-chevron-up"></span>
        Like <i class="small material-icons">thumb_up</i>
        <%= post.get_upvotes.size %>
      <% end %>
      <%= link_to dislike_post_path(post), method: :put, class: "btn btn-default btn-sm" do %>
        <span class="glyphicon glyphicon-chevron-down">
        Dislike <i class="small material-icons">thumb_down</i>
        <%= post.get_downvotes.size %>
      <% end %>
    </div>
   <div class="small material-icons", style="padding-top: 4%;">comment</div>
 <%= post.comments.count %> Comment(s)
 <div>
 <i class="small material-icons">insert_chart</i>
<%= "#{post.impressionist_count} views" %>
</div>
  <div calss="saveme">
  </div>
  </div>
 
 </div>


 
<% end %>
<% end %>