       <li><%= link_to(t("layouts.log_in"), new_user_session_path, class: "btn waves-effect waves-light") %></li> 

  <li><%= link_to "Sign in with Facebook", user_omniauth_authorize_path(:facebook), class: "btn waves-effect waves-light" %>
  <li><%= link_to "Sign in with Vkontakte", user_omniauth_authorize_path(:vkontakte), class: "btn waves-effect waves-light" %>

</li>

      </ul>
    </div>
  </nav>
 </div>
      <div id="wrapper" class="toggled">

      
        <div id="sidebar-wrapper">
            <ul class="sidebar-nav">
